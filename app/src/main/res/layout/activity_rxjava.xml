<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".rxjava.RXJavaActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="getBanner"
            android:padding="10dp"
            android:text="getBanner" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="test"
            android:padding="10dp"
            android:text="test" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="mapTest"
            android:padding="10dp"
            android:text="mapTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="zipTest"
            android:padding="10dp"
            android:text="zipTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="concatTest"
            android:padding="10dp"
            android:text="concatTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="flatMapTest"
            android:padding="10dp"
            android:text="flatMapTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="concatMapTest"
            android:padding="10dp"
            android:text="concatMapTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="distinctTest"
            android:padding="10dp"
            android:text="distinctTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="filterTest"
            android:padding="10dp"
            android:text="filterTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="bufferTest"
            android:padding="10dp"
            android:text="bufferTest" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="RxJavaUtil"
            android:padding="10dp"
            android:text="RxJavaUtil" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="subscribeOnMethodTest"
            android:padding="10dp"
            android:text="subscribeOnMethodTest"
            tools:ignore="OnClick" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="flatMapTest1"
            android:padding="10dp"
            android:text="flatMapTest1"
            tools:ignore="OnClick" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="flatMapTest2"
            android:padding="10dp"
            android:text="flatMapTest2"
            tools:ignore="OnClick" />

    </LinearLayout>
</ScrollView>